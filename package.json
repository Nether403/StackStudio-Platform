{
  "name": "stackfast-mvp",
  "version": "3.0.0",
  "description": "StackFast - AI-powered tech stack recommendations with authentication and user management",
  "main": "index.js",
  "scripts": {
    "dev": "node -r ts-node/register scripts/check-env.ts && next dev",
    "build": "node -r ts-node/register scripts/check-env.ts && next build",
    "start": "node -r ts-node/register scripts/check-env.ts && next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "sync-db": "node Scripts/sync-firestore.js",
    "test-github": "node test-github-integration.js",
    "deploy-prep": "pwsh ./deploy-prep.ps1",
    "check-env": "node -r ts-node/register scripts/check-env.ts",
    "env-template": "node -r ts-node/register scripts/check-env.ts --template"
  },
  "dependencies": {
    "@auth/firebase-adapter": "^2.10.0",
    "@google-cloud/firestore": "^7.1.0",
    "@octokit/rest": "^22.0.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@vercel/node": "^3.0.0",
    "autoprefixer": "^10.4.16",
    "chart.js": "^4.5.0",
    "firebase": "^10.7.0",
    "firebase-admin": "^12.7.0",
    "next": "^14.0.0",
    "next-auth": "^4.24.11",
    "postcss": "^8.4.31",
    "react": "^18.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "tech-stack",
    "ai-recommendations",
    "developer-tools",
    "mvp-builder"
  ],
  "author": "StackStudio",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
